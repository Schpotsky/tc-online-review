    <repository>
      <id>central</id>
      <url>https://repo.maven.apache.org/maven2</url>
    </repository>
    <repository>
      <id>temp-maven-repo</id>
      <url>https://github.com/coderReview/temp-maven-repo/raw/master</url>
      <snapshots>
        <enabled>true</enabled>
        <updatePolicy>always</updatePolicy>
      </snapshots>
    </repository>
    <repository>
      <id>JBoss repository</id>
      <url>https://repository.jboss.org</url>
    </repository>
    <repository>
      <id>JBoss old repository</id>
      <url>https://maven-us.nuxeo.org/nexus/content/groups/public</url>
    </repository>
    <repository>
      <id>Spring Plugins</id>
      <url>http://repo.spring.io/plugins-release</url>
    </repository>
  </repositories>

  <pluginRepositories>
    <pluginRepository>
      <id>temp-maven-repo</id>
      <url>https://github.com/coderReview/temp-maven-repo/raw/master</url>
      <snapshots>
        <enabled>true</enabled>
        <updatePolicy>always</updatePolicy>
      </snapshots>
    </pluginRepository>
  </pluginRepositories>

  <build>
    <finalName>review</finalName>
    <pluginManagement>
      <plugins>
        <plugin>
          <artifactId>maven-clean-plugin</artifactId>
          <version>3.0.0</version>
        </plugin>
        <plugin>
          <artifactId>maven-compiler-plugin</artifactId>
          <version>3.7.0</version>
          <configuration>
            <source>1.5</source>
            <target>1.5</target>
          </configuration>
        </plugin>
        <plugin>
          <artifactId>maven-antrun-plugin</artifactId>
          <version>1.8</version>
          <executions>
            <execution>
              <id>tokenize</id>
              <configuration>
                <target>
                  <ant antfile="${basedir}/build.pom.xml" target="tokenize"/>
                </target>
              </configuration>
              <goals>
                <goal>run</goal>
              </goals>
            </execution>
            <execution>
              <id>tokenize_all</id>
              <configuration>
                <target>
                  <ant antfile="${basedir}/build.pom.xml" target="tokenize_all"/>
                </target>
              </configuration>
              <goals>
                <goal>run</goal>
              </goals>
            </execution>
            <execution>
              <id>deploy-locally</id>
              <configuration>
                <target>
                  <ant antfile="${basedir}/build.pom.xml" target="copy_shared_files"/>
                </target>
              </configuration>
              <goals>
                <goal>run</goal>
              </goals>
            </execution>
            <execution>
              <id>first-deploy</id>
              <configuration>
                <target>
                  <ant antfile="${basedir}/build.pom.xml" target="first_deploy"/>
                </target>
              </configuration>
              <goals>
                <goal>run</goal>
              </goals>
            </execution>
          </executions>
        </plugin>
        <plugin>
          <artifactId>maven-war-plugin</artifactId>
          <version>3.2.2</version>
          <configuration>
            <archive>
              <manifestEntries>
                <Component-Vendor>Topcoder Inc.</Component-Vendor>
                <Component-Name>${project.artifactId}</Component-Name>
                <Component-Version>${project.version}</Component-Version>
              </manifestEntries>
            </archive>
            <webResources>
              <resource>
                <directory>${output.dir}/conf/WEB-INF</directory>
                <targetPath>WEB-INF</targetPath>
              </resource>
              <resource>
                <directory>${output.dir}/conf/META-INF</directory>
                <targetPath>META-INF</targetPath>
              </resource>
              <resource>
                <directory>${output.dir}/conf</directory>
                <targetPath>WEB-INF/classes</targetPath>
                <excludes>
                  <exclude>WEB-INF/**</exclude>
                  <exclude>META-INF/**</exclude>
                  <exclude>*.war</exclude>
                </excludes>
              </resource>
            </webResources>
            <outputDirectory>${output.dir}</outputDirectory>
          </configuration>
        </plugin>
        <plugin>
          <groupId>com.github.nikodem911</groupId>
          <artifactId>s3-upload-maven-plugin</artifactId>
          <version>1.5.1</version>
          <configuration>
            <bucketName>${s3.bucket.name}</bucketName>
            <source>${output.dir}/</source>
            <destination>online_review/</destination>
            <recursive>true</recursive>
          </configuration>
        </plugin>
        <plugin>
          <groupId>com.coderreview.or</groupId>
          <artifactId>s3-download-maven-plugin</artifactId>
          <version>1.0.1</version>
          <configuration>
            <bucketName>${s3.bucket.name}</bucketName>
            <source>token.properties</source>
            <destination>./</destination>
          </configuration>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>
